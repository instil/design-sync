{
  "name": "figma-sync",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn checks",
    "checks": "yarn lint && yarn test",
    "lint": "yarn eslint --max-warnings=0 \"./src/**/*.{ts,tsx}\"",
    "test": "yarn jest --passWithNoTests",
    "sync": "yarn ts-node -r tsconfig-paths/register src/SyncFigmaWithoutIcons.ts",
    "sync:icons": "yarn ts-node -r tsconfig-paths/registersrc/ SyncFigmaWithIcons.ts"
  },
  "devDependencies": {
    "@types/jest": "27.5.1",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.12.0",
    "axios": "0.27.2",
    "dotenv": "16.0.1",
    "eslint": "8.9.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.4.5",
    "eslint-plugin-no-relative-import-paths": "1.3.3",
    "figma-api": "1.10.1",
    "jest": "28.1.0",
    "lodash": "4.17.21",
    "style-dictionary": "3.7.1",
    "ts-jest": "28.0.3",
    "ts-node": "10.8.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.2",
    "typescript-transform-paths": "3.3.1"
  }
}
